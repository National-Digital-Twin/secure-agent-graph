POST {{SAG_SERVER}}/ds
Authorization: bearer {{ID_TOKEN_USER_1}}
[FormParams]
query: select ?s ?p ?o where { ?s ?p ?o . }
HTTP 200
[Asserts]
jsonpath "$.results.bindings[*].s.value" not contains "{{USER_2_DATA}}"
jsonpath "$.results.bindings[*].s.value" contains "{{USER_1_DATA}}"


POST {{SAG_SERVER}}/ds
Authorization: bearer {{ID_TOKEN_USER_2}}
[FormParams]
query: select ?s ?p ?o where { ?s ?p ?o . }
HTTP 200
[Asserts]
jsonpath "$.results.bindings[*].s.value" contains "{{USER_1_DATA}}"
jsonpath "$.results.bindings[*].s.value" contains "{{USER_2_DATA}}"
